import java.util.Scanner;

public class print_till_n_using_functions {
 
	
	public static void printTillN(int n) {
		
		for (int i = 1 ; i <= n ; i++) {
			System.out.println(i);
		}
	}
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		  printTillN(n);
		  System.out.println();
		

	}

}
