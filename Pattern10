import java.util.Scanner;

public class pattern_1 {

	public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
	int n = scan.nextInt();
	int i = 1;
	while (i <= n) {
		int j = 1;
		while(j <= n) {
			System.out.print(j + " ");
			j++;
			if (j > i) {
				break;
			}
		}System.out.println();
		i++;
	}

	}

}

/* The Pattern Will Be As Follows 
 * 1
 * 12
 * 123
 * 1234
 * 12345
*/