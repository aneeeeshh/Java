public class Linear_Search {
	
	public static int linearSearch(int input[], int elem) {
		int start = 0;
		int end = input.length - 1;
		
		for (int i = 0; i< end; i++) {
			if (input[i] == elem) {
				System.out.println(i+ "th index");
				return 1;
				
			}
			
		}return -1;
	}

	public static void main(String[] args) {
		int input[] = {12,23,34,12,56,34,78,56,98};
		int index = linearSearch(input, 56);
		if (index == 1) {
			System.out.println("Element is In the array");
		}
		else {
			System.out.println("Element not found");
		}
				System.out.println();

	}

}
/* This Program Searches The Element By Linear Search
 But The Array And Element Is Given In The Code only */