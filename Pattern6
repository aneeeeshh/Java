import java.util.Scanner;

public class pattern_6 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int i = 1;
		while (i <= n) {
			int j = 1;
			while (j <= n) {
				System.out.print((char) ('A' + j - 1));
				j++;
			}System.out.println();
			i++;
		}
	
	}

}

/*The Pattern Will Be As Follows
 * ABCDE
 * ABCDE
 * ABCDE
 * ABCDE
 * ABCDE
 */